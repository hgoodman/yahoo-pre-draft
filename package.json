{
  "name": "yahoo-pre-draft",
  "version": "0.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hgoodman/yahoo-pre-draft.git"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.4",
    "@types/jquery": "^3.5.32",
    "eslint": "^8.38.0",
    "eslint-config-google": "^0.7.1",
    "gulp": "^5.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-eslint": "^6.0.0",
    "gulp-jasmine": "^4.0.0",
    "gulp-zip": "^5.1.0",
    "jasmine": "^5.9.0"
  },
  "eslintConfig": {
    "extends": "google",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "comma-dangle": "off",
      "key-spacing": [
        "error",
        {
          "mode": "minimum"
        }
      ],
      "no-multi-spaces": [
        "error",
        {
          "exceptions": {
            "ObjectExpression": true,
            "SwitchCase": true
          }
        }
      ],
      "no-var": "off",
      "require-jsdoc": "off",
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "linebreak-style": 0
    }
  }
}
